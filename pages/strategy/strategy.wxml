
<!-- //  wxSearch作者提供的模板 -->
<import src="../../wxSearch/wxSearch/wxSearch.wxml"/>
<view class="wxSearch-section">
      <view class="wxSearch-pancel">
          <input bindinput="wxSearchInput" bindfocus="wxSerchFocus" value="{{wxSearchData.value}}"  bindblur="wxSearchBlur" class="wxSearch-input" placeholder="搜索" bindconfirm="wxConfirm" confirm-type="search"/>
          <button class="wxSearch-button" bindtap="wxSearchFn" size="mini" plain="true">搜索</button>
      </view>
</view>
<template is="wxSearch" data="{{wxSearchData}}"/>


